    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug executable 'te'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "run",
                    "--bin=te"
                ]
            },
            "args": [
                "gcloud",
                "alpha",
                "pam",
                "grants",
                "create",
                "--entitlement",
                "secret-manager-admin",
                "--requested-duration",
                "28800s",
                "--justification",
                "dev",
                "--location",
                "global",
                "--project",
                "my-project"
            ]
        },
        {
            "name": "Debug unit tests in executable 'te'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--bin=te"
                ]
            }
        }
    ]
}
